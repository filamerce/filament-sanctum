{
    "name": "filamerce/filament-sanctum",
    "description": "Filament Sanctum",
    "keywords": [
        "Devtical",
        "sanctum",
        "laravel"
    ],
    "homepage": "https://github.com/filamerce/filament-sanctum",
    "license": "MIT",
    "authors": [
        {
            "name": "Wahyu Kristianto",
            "email": "w.kristories@gmail.com"
        }
    ],
    "require": {
        "filament/filament": "^3.0",
        "laravel/sanctum": "^4.0",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^3.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Devtical\\Sanctum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devtical\\Sanctum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtical\\Sanctum\\SanctumServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
